{
  "name": "sbb-angular-showcase",
  "version": "0.2.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "clean": "rimraf dist coverage lintReport.json",
    "build": "run-s build:library build:showcase",
    "build:library": "run-s build:library:ng build:library:styles build:library:typography",
    "build:library:ng": "ng build sbb-angular",
    "build:library:styles": "scss-bundle -e ./projects/sbb-angular/src/styles/main.scss -d ./dist/sbb-angular/styles.scss",
    "build:library:typography": "node-sass ./projects/sbb-angular/src/typography.scss ./dist/sbb-angular/typography.css",
    "build:icons": "node ./scripts/create-svg-icons-components.js",
    "build:showcase": "ng build sbb-angular-showcase --prod",
    "test": "run-s build:library test:*",
    "test:showcase": "ng test sbb-angular-showcase -c ci",
    "test:library": "ng test sbb-angular -c ci",
    "lint": "run-s lint:*",
    "lint:showcase": "ng lint sbb-angular-showcase",
    "lint:library": "run-s lint:library:*",
    "lint:library:console": "ng lint sbb-angular",
    "lint:library:report": "ng lint sbb-angular --format=json > lintReport.json",
    "sbb:publish": "node scripts/publish.js",
    "e2e": "ng e2e",
    "docs": "gulp docs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.7",
    "@angular/cdk": "^6.4.7",
    "@angular/common": "^6.1.7",
    "@angular/compiler": "^6.1.7",
    "@angular/core": "^6.1.7",
    "@angular/forms": "^6.1.7",
    "@angular/http": "^6.1.7",
    "@angular/platform-browser": "^6.1.7",
    "@angular/platform-browser-dynamic": "^6.1.7",
    "@angular/router": "^6.1.7",
    "@ng-bootstrap/ng-bootstrap": "^3.2.0",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "ngx-monaco-editor": "^6.0.0",
    "ngx-perfect-scrollbar": "^6.3.1",
    "rxjs": "~6.2.0",
    "util": "^0.10.4",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.1",
    "@angular-devkit/build-ng-packagr": "^0.8.1",
    "@angular/cli": "^6.2.1",
    "@angular/compiler-cli": "^6.1.7",
    "@angular/language-service": "^6.1.7",
    "@types/gulp": "^4.0.5",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.4",
    "@types/node": "^8.10.37",
    "@types/run-sequence": "^0.0.30",
    "codelyzer": "~4.3.0",
    "dgeni": "^0.4.10",
    "dgeni-packages": "^0.26.12",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-clean-css": "^3.10.0",
    "gulp-cli": "^2.0.1",
    "gulp-connect": "^5.6.1",
    "gulp-conventional-changelog": "^2.0.9",
    "gulp-dom": "^1.0.0",
    "gulp-flatten": "^0.4.0",
    "gulp-highlight-files": "^0.0.6",
    "gulp-html-beautify": "^1.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^2.0.2",
    "gulp-markdown": "^3.0.0",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-transform": "^3.0.5",
    "gulp-util": "^3.0.8",
    "highlight.js": "^9.13.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.3",
    "karma-firefox-launcher": "^1.1.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "lodash": "^4.17.11",
    "ng-packagr": "^4.2.0",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "protractor": "^5.4.1",
    "run-sequence": "^2.2.1",
    "scss-bundle": "^2.4.0",
    "semver": "^5.5.1",
    "svgo": "^1.0.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "tsickle": "^0.33.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
