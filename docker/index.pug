html
  head
    title SBB Angular Component Library Versions
    link(
      rel="stylesheet"
      type="text/css"
      href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"
      integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u"
      crossorigin="anonymous"
    )
  body
    div(class="container")
      div(class="row")
        h1 SBB Angular Component Library Versions
        table(class="table")
          each version in showcaseVersions
            tr
              td
                a(href='/' + version.target)= version
              td= version.toLocaleDatetime()
